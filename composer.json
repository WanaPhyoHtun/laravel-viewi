{
    "name": "protonemedia/laravel-viewi",
    "description": "Integrates Viewi with Laravel",
    "keywords": [
        "protonemedia",
        "laravel",
        "laravel-viewi"
    ],
    "homepage": "https://github.com/protonemedia/laravel-viewi",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73",
        "illuminate/filesystem": "^8.73",
        "viewi/viewi": "0.1.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\LaravelViewi\\": "src",
            "ProtoneMedia\\LaravelViewi\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\LaravelViewi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\LaravelViewi\\LaravelViewiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}